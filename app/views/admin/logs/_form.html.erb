<div class="col-sm-6">
<%= simple_form_for([:admin, @log], :html => {class: 'form-horizontal', role: 'form'}) do |f| %>
  <%= f.error_notification %>

    
  <div class="form-group">
    <%= f.input :uniqid, placeholder: 'Uniqid', input_html: { class: 'form-control' } %>
  </div>
        
  <div class="form-group">
    <%= f.association :contact, label_method: :name, value_method: :id, include_blank: true, input_html: { class: 'form-control' } %>
  </div>
        
  <div class="form-group">
    <%= f.input :log_type, placeholder: 'Log type', input_html: { class: 'form-control' } %>
  </div>
        
  <div class="form-group">
    <%= f.input :time, placeholder: 'Time', input_html: { class: 'form-control' } %>
  </div>
        
  <div class="form-group">
    <%= f.input :duration, placeholder: 'Duration', input_html: { class: 'form-control' } %>
  </div>
        
  <div class="form-group">
    references
    <%= f.association :device, label_method: :name, value_method: :id, include_blank: true, input_html: { class: 'form-control' } %>
  </div>
    
  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-success' %>
  </div>
<% end %>
</div>